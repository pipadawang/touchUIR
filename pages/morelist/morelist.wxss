page{
  height: 100%
}
.card {
    border: 5px solid #ffffff;
    border-radius: 5px;
    background-color: #ffffff;
    box-shadow: 6px 2px 10px #cccccc;
    margin: 8px;
    position: relative;
}
.title{
  font-size: 22px;
  margin-left: 4%;
  margin-right:4%;
  color: rgb(49, 48, 46)

}
.image {
    width: 100%;
    height: 200px;
    background-color: #eeeeee;
    filter: blur(10rpx)
}

.time {
    float: right;
    margin-top: 170px;
    color: #ffffff;
    position: absolute;
    right: 0;
    top: 0;
    z-index: 1;
    font-size: 12px;
    margin-right: 3%
}
.im{
  overflow: hidden
}
.titlecave{
  height: 20%;
  width: 100%
}